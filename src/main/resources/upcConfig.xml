            <menuItem>
                <name>用户管理</name>
                <uri>/acl/user/manager</uri>
                <path></path>
            </menuItem>
			<menuItem>
				<name>机构管理</name>
				<uri>/acl/organize/manager</uri>
				<path></path>
			</menuItem>
		</menu>
		<menu name="角色与岗位管理">
            <menuItem>
                <name>用户岗位管理</name>
                <uri>/acl/station/manager</uri>
                <path></path>
            </menuItem>
			<menuItem>
				<name>角色管理</name>
				<uri>/acl/role/manager</uri>
                <path></path>
			</menuItem>
		</menu>
		<menu name="资源管理">
			<menuItem>
				<name>菜单资源管理</name>
				<uri>/acl/menuResource/manager</uri>
                <path></path>
			</menuItem>
			<menuItem>
				<name>数据资源管理</name>
